package javaStudy;

public class StringExam {

	public static void main(String[] args) {

		String str1 = "Hello";
		String str2 = "Hello";
		
		String str3 = new String("Hello");
		String str4 = new String("Hello");
		
		if (str1 == str2) {
			System.out.println("str1과 str2는 같은 레퍼런스입니다.");
		}
		
		if (str1 == str3) {
			System.out.println("str1과 str2는 같은 레퍼런스입니다.");
		}
	}

}
